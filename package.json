{
  "name": "project-template-back",
  "version": "1.0.0",
  "author": "New York Style",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "dev": "nest start --watch",
    "dev:debug": "nest start --debug --watch",
    "prod": "node dist/src/main",
    "prettier:check": "prettier --check \"**/*.{ts,js,md,mdx,yaml}\"",
    "prettier:format": "prettier --write \"**/*.{ts,js,md,mdx,yaml}\"",
    "migrate": "ts-node db_migration/migrate.ts",
    "semantic-release": "semantic-release",
    "semantic-release:dry-run": "semantic-release --dry-run",
    "codestyle:check": "npm run prettier:check && npm run lint"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "npx pretty-quick --staged",
      "npx eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@nestjs/common": "10.0.0",
    "@nestjs/config": "3.3.0",
    "@nestjs/core": "10.4.15",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/passport": "10.0.3",
    "@nestjs/platform-express": "10.4.15",
    "@nestjs/swagger": "8.1.0",
    "@nestjs/typeorm": "10.0.2",
    "argon2": "0.41.1",
    "cookie-parser": "1.4.7",
    "dotenv": "16.4.7",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "passport-local": "1.0.0",
    "pg": "8.13.1",
    "typeorm": "0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "10.4.9",
    "@nestjs/schematics": "10.0.0",
    "@nestjs/testing": "10.0.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "11.0.6",
    "@semantic-release/npm": "12.0.2",
    "@types/cookie-parser": "1.4.8",
    "@types/express": "5.0.0",
    "@types/jest": "29.5.2",
    "@types/node": "20.3.1",
    "@types/passport-jwt": "4.0.1",
    "@types/passport-local": "1.0.38",
    "@types/pg": "8.11.11",
    "@types/supertest": "6.0.0",
    "@typescript-eslint/eslint-plugin": "8.0.0",
    "@typescript-eslint/parser": "8.0.0",
    "commitizen": "4.3.1",
    "cz-conventional-changelog": "3.3.0",
    "conventional-changelog-conventionalcommits": "9.1.0",
    "eslint": "9.8.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.3",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-sort-destructure-keys": "2.0.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "lint-staged": "15.2.10",
    "prettier": "3.4.2",
    "pretty-quick": "4.0.0",
    "semantic-release": "24.2.9",
    "source-map-support": "0.5.21",
    "supertest": "7.0.0",
    "ts-jest": "29.1.0",
    "ts-loader": "9.4.3",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.5.2",
    "typescript-eslint": "8.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
